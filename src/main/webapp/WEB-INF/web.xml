<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>board-server</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>join</servlet-name>
    <jsp-file>/views/user/joinForm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>join</servlet-name>
    <url-pattern>/join</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>joinPro</servlet-name>
    <jsp-file>/views/user/joinProForm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>joinPro</servlet-name>
    <url-pattern>/joinPro</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/views/user/loginForm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>loginPro</servlet-name>
    <jsp-file>/views/user/loginFormPro.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginPro</servlet-name>
    <url-pattern>/loginPro</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logoutPro</servlet-name>
    <jsp-file>/views/user/logoutPro.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>logoutPro</servlet-name>
    <url-pattern>/logoutPro</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>myPage</servlet-name>
    <jsp-file>/views/user/myPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>myPage</servlet-name>
    <url-pattern>/myPage</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>updateUser</servlet-name>
    <jsp-file>/views/user/updateUserForm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateUser</servlet-name>
    <url-pattern>/updateUser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>updateUserPro</servlet-name>
    <jsp-file>/views/user/updateUserFormPro.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateUserPro</servlet-name>
    <url-pattern>/updateUserPro</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>deleteUser</servlet-name>
    <jsp-file>/views/user/deleteUserForm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>deleteUser</servlet-name>
    <url-pattern>/deleteUser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>deleteUserPro</servlet-name>
    <jsp-file>/views/user/deleteUserFormPro.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>deleteUserPro</servlet-name>
    <url-pattern>/deleteUserPro</url-pattern>
  </servlet-mapping>

</web-app>